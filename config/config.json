{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "categorias-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/categorias/CategoriasWebPart.js",
          "manifest": "./src/webparts/categorias/CategoriasWebPart.manifest.json"
        }
      ]
    },
    "tickets-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/tickets/TicketsWebPart.js",
          "manifest": "./src/webparts/tickets/TicketsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CategoriasWebPartStrings": "lib/webparts/categorias/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "TicketsWebPartStrings": "lib/webparts/tickets/loc/{locale}.js"
  }
}
